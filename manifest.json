{
  "manifest_version": 3,
  "name": "ClickToLens",
  "version": "1.0",
  "description": "Search any image with Google Lens and other search engines",
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
